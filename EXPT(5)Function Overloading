#include <iostream>

using namespace std;

int add(int a, int b) {
    return a + b;
}

double add(double a, double b) {
    return a + b;
}

int main() {
    int ResultInt = add(5, 10);
    cout << "Result of adding integers: " << ResultInt << endl;

    double ResultDouble = add(5.2, 10.5);
    cout << "Result of adding doubles: " << ResultDouble << endl;

    return 0;
}
